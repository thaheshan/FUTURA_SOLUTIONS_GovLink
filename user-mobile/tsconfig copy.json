{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@*": ["src/*"],
      "@store": ["src/store/index"],
      "@store/*": ["../Client-Mobile/src/Store*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "src/lib/config.js"
  ]
}
